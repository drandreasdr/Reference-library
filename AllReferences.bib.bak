@Article{Kabo2010,
  Title                    = {Rolling contact fatigue prediction for rails and comparisons with test rig results},
  Author                   = {Kabo, E and Ekberg, A and Torstensson, P. T and Vernersson, T},
  Journal                  = {Proceedings of the Institution of Mechanical Engineers, Part F: Journal of Rail and Rapid Transit},
  Year                     = {2010},
