@Article{Maouche1997,
  Title                    = {On a new method of evaluation of the inelastic state due to moving contacts},
  Author                   = {N. Maouche and M. H. Maitournam and K. Dang Van},
  Journal                  = {Wear},
  Year                     = {1997},
  Note                     = {11th International Conference on Wear of Materials</ce:title>},
  Number                   = {0},
  Pages                    = {139--147},
  Volume                   = {203--204},

  Abstract                 = {The prediction of damage and wear of materials under repeated moving contacts requires first the determination of the limit response in terms of stress and strain. The simulation necessary to calculate the limit response could be performed with classical finite elements and an incremental treatment of the loading history, resulting in lenghtly are repeated calculations. For steady-state moving contacts, the stationary methods allows a quick evaluation of the stabilized state. These procedures cannot be used in the case of an arbitrary cyclic loading resulting, for example, from small oscillatory contacts. In this paper, a new finite element approach is developed for structures subjected to an arbitrary cyclic loading. It is based on an original intergration scheme which allows direct determination of the stabilized state of stresses, strains and internal parameters. This asymptotic state could be either an elastic shakedown, a plastic shakedown or ratchetting for an elastic-plastic material. Elastic and plastic shakedown states are easily obtained with this method. Applications to fretting fatigue and wear in relation to material fatigue properties are presented.},
  Doi                      = {10.1016/S0043-1648(96)07428-5},
  ISSN                     = {0043-1648},
  Keywords                 = {Numerical method},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0043164896074285}
}

