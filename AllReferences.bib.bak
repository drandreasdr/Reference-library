@Article{HughesFeijoo1998,
  Title                    = {The variational multiscale method --- a paradigm for computational mechanics},
  Author                   = {Thomas J.R. Hughes and Gonzalo R. Feijoo and Luca Mazzei and Jean-Baptiste Quincy},
