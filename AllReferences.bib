@INCOLLECTION{TemizerWriggers2011incol,
  author = {Temizer, \'{I}lker and Wriggers, Peter},
  title = {Finite Deformation Thermomechanical Contact Homogenization Framework},
  booktitle = {Trends in Computational Contact Mechanics},
  publisher = {Springer Berlin / Heidelberg},
  year = {2011},
  editor = {Zavarise, Giorgio and Wriggers, Peter},
  volume = {58},
  series = {Lecture Notes in Applied and Computational Mechanics},
  pages = {101--119},
  abstract = {A finite deformation homogenization framework is developed to predict
	the macroscopic thermal response of contact interfaces between rough
	surface topographies. The overall homogenization framework transfers
	macroscopic contact variables such as surfacial stretch, pressure
	and heat flux as boundary conditions on a test sample within a micromechanical
	interface testing procedure. An analysis of the thermal dissipation
	within the test sample reveals a thermodynamically consistent identification
	for the macroscopic thermal contact conductance parameter that enables
	the solution of a homogenized thermomechanical contact boundary value
	problem based on standard computational approaches. The homogenized
	contact response effectively predicts a temperature jump across the
	macroscale contact interface.},
  affiliation = {Institute of Continuum Mechanics, Leibniz University of Hannover,
	Appelstr. 11, 30169 Hannover, Germany},
  doi = {10.1007/978-3-642-22167-5_6},
  isbn = {978-3-642-22166-8},
  keyword = {Engineering},
  url = {http://dx.doi.org/10.1007/978-3-642-22167-5_6}
}

@ARTICLE{TemizerWriggers2010,
  author = {Temizer, \'{I}. and Wriggers, P.},
  title = {Thermal contact conductance characterization via computational contact
	homogenization: A finite deformation theory framework},
  journal = {International Journal for Numerical Methods in Engineering},
  year = {2010},
  volume = {83},
  pages = {27--58},
  number = {1},
  abstract = {In order to predict the macroscopic thermal response of contact interfaces
	between rough surface topographies, a computational contact homogenization
	technique is developed at the finite deformation regime. The overall
	homogenization framework transfers macroscopic contact variables,
	such as surfacial stretch, pressure and heat flux, as boundary conditions
	on a test sample within a micromechanical interface testing procedure.
	An analysis of the thermal dissipation within the test sample reveals
	a thermodynamically consistent identification for the macroscopic
	thermal contact conductance parameter that enables the solution of
	a homogenized thermomechanical contact boundary value problem based
	on standard computational approaches. The homogenized contact response
	effectively predicts a temperature jump across the macroscale contact
	interface. The strong dependence of this homogenized response on
	macroscale solution variables of interest is demonstrated via representative
	three-dimensional numerical investigations. The proposed contact
	homogenization framework is suitable for the analysis of similar
	energy transport phenomena across heterogeneous contact interfaces
	where the investigation of the sources for energy dissipation is
	of concern.},
  doi = {10.1002/nme.2822},
  issn = {1097-0207},
  keywords = {contact homogenization, contact conductance, finite deformation},
  publisher = {John Wiley \& Sons, Ltd.},
  url = {http://dx.doi.org/10.1002/nme.2822}
}

